{% extends('partials/_master.twig') %}

{% setcontent apropos = 'about' %}
{% setcontent features = 'features' %}

{% block main %}
    <!--Page Header Start-->
    <section class="page-header" style="background-image: url({{ apropos.background|thumbnail }});">
        <div class="page-header-shape-1"></div>
        <div class="page-header-shape-2"></div>
        <div class="container">
            <div class="page-header__inner">
                <ul class="thm-breadcrumb list-unstyled">
                    <li><a href="{{ path('homepage') }}">Acceuil</a></li>
                    <li><span>.</span></li>
                    <li>{{ apropos.title ?? '' }}</li>
                </ul>
                <h2>{{ apropos.title ?? '' }}</h2>
            </div>
        </div>
    </section>
    <!--Page Header End-->

    <!--Reasons Start-->
    <section class="reasons">
        <div class="container">
            <div class="row">
                <div class="col-xl-7 col-lg-6">
                    <div class="reasons__left">
                        <div class="reasons__img">
                            <img src="{{ apropos.image|thumbnail }}" alt="">
                            <div class="reasons__img-shape-1"></div>
                            <div class="reasons__img-shape-2"></div>
                        </div>
                    </div>
                </div>
                <div class="col-xl-5 col-lg-6">
                    <div class="reasons__right">
                        <div class="section-title text-left">
                            <span class="section-title__tagline">{{ apropos.tag ?? '' }}</span>
                            <h2 class="section-title__title">{{ apropos.whyUs ?? '' }}</h2>
                        </div>
                        <p class="reasons__text">{{ apropos.body ?? '' }}</p>
                        <ul class="list-unstyled reasons__list">
                            <li>
                                <div class="icon">
                                    <span class="icon-tick"></span>
                                </div>
                                <div class="text">
                                    <p>{{ features.feature1 ?? '' }}</p>
                                </div>
                            </li>
                            <li>
                                <div class="icon">
                                    <span class="icon-tick"></span>
                                </div>
                                <div class="text">
                                    <p>{{ features.feature2 ?? '' }}</p>
                                </div>
                            </li>
                            <li>
                                <div class="icon">
                                    <span class="icon-tick"></span>
                                </div>
                                <div class="text">
                                    <p>{{ features.feature3 ?? '' }}</p>
                                </div>
                            </li>
                        </ul>
                        <a href="{{ features.button_link ?? '#' }}" class="thm-btn reasons__btn"><span>{{ features.button_text ?? 'Contactez-nous'}}</span></a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--Reasons End-->

    <!--Counter One Start-->
    {{ include('partials/_counter.twig') }}
    <!--Counter One End-->

    <!--We Change Start-->
    {% include('partials/_faq.twig') %}
